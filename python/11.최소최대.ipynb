   "source": [
    "\r\n",
    "K = list ( map(int, input().split()) )\r\n",
    "\r\n",
    "def gcd(a, b) :\r\n",
    "    c = a%b\r\n",
    "    while c:\r\n",
    "        a = b \r\n",
    "        b = c\r\n",
    "        c = a%b \r\n",
    "    return b\r\n",
    "\r\n",
    "min_acc = gcd( max(K), min(K)  )\r\n",
    "\r\n",
    "print(min_acc)\r\n",
    "print( int ( K[0]*K[1]/min_acc) )\r\n",
    "\r\n"
   ],
