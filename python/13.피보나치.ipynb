    "def dp(n) :\r\n",
    "    if n == 1 or n == 0 : return n\r\n",
    "    # if n == 0 : return 0\r\n",
    "    return dp(n-1) + dp(n-2)\r\n",
    "\r\n",
    "n = int( input() )\r\n",
    "\r\n",
    "print(dp(n)) "
