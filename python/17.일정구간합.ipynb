    "\r\n",
    "def dfs(n) :\r\n",
    "    if n == 1 :\r\n",
    "        return [1]\r\n",
    "    return dfs(n-1) + [ n for i in range(n)]\r\n",
    "\r\n",
    "N = dfs(45)\r\n",
    "x, y = map(int, input().split())\r\n",
    "ans = 0\r\n",
    "for i in range(x, y+1) :\r\n",
    "    ans += N[i-1]\r\n",
    "print(ans)"
